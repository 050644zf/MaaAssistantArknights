            "template": "Loading.png",
            "threshold": 0.8,
            "type": "doNothing",
            "next": [
                "StartButton1"
            ]
        },
        "UseMedicine": {
            "template": "UseMedicine.png",
            "type": "doNothing",
            "next": [
                "MedicineConfirm"
            ]
        },
        "MedicineConfirm": {
            "template": "MedicineConfirm.png",
            "type": "clickSelf",
            "reduceOtherTimes": [
                "StartButton1"
            ],
            "next": [
                "StartButton1"
            ]
        },
        "UseStone": {
            "template": "UseStone.png",
            "type": "doNothing",
            "next": [
                "StoneConfirm"
            ]
        },
        "StoneConfirm": {
